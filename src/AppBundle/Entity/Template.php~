namespace AppBundle\Entity;

use Doctrine\Common\Collections\ArrayCollection;
use Doctrine\ORM\Mapping as ORM;

/**
 * Class Template
 * @package AppBundle\Entity
 * @author Szymon Kunowski <szymon.kunowski@gmail.com>
 *
 * @ORM\Entity
 * @ORM\Table(name="template")
 */
class Template
{
    /**
     * @ORM\Id
     * @ORM\Column(type="integer")
     * @ORM\GeneratedValue(strategy="AUTO")
     * @var int
     */
    protected $id;


    /**
     * @var ArrayCollection
     * @ORM\OneToMany(targetEntity="Talk", mappedBy="template")
     */
    protected $talks;

    /**
     * @var ArrayCollection
     * @ORM\OneToMany(targetEntity="Scene", mappedBy="template")
     */
    protected $scenes;

    /**
     * @var Video
     * @ORM\ManyToOne(targetEntity="Video", inversedBy="templates")
     */
    protected $video;

    public function __construct()
    {

    }



}
